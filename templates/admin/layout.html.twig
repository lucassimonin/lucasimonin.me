<!DOCTYPE HTML>
<html class="gt-ie8 gt-ie9 not-ie" lang="{{ app.request.locale }}">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}{% endblock %} | {{ 'admin.adminstration.label'|trans({}, 'app') }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('images/front/apple-touch-icon-57x57.png') }}" />
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('images/front/apple-touch-icon-114x114.png') }}" />
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('images/front/apple-touch-icon-72x72.png') }}" />
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('images/front/apple-touch-icon-144x144.png') }}" />
    <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('images/front/apple-touch-icon-60x60.png') }}" />
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('images/front/apple-touch-icon-120x120.png') }}" />
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('images/front/apple-touch-icon-76x76.png') }}" />
    <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('images/front/apple-touch-icon-152x152.png') }}" />
    <link rel="icon" type="image/png" href="{{ asset('images/front/favicon-196x196.png') }}" sizes="196x196" />
    <link rel="icon" type="image/png" href="{{ asset('images/front/favicon-96x96.png') }}" sizes="96x96" />
    <link rel="icon" type="image/png" href="{{ asset('images/front/favicon-32x32.png') }}" sizes="32x32" />
    <link rel="icon" type="image/png" href="{{ asset('images/front/favicon-16x16.png') }}" sizes="16x16" />
    <link rel="icon" type="image/png" href="{{ asset('images/front/favicon-128.png') }}" sizes="128x128" />
    {% block stylesheets %}
        {{ encore_entry_link_tags('admin') }}
    {% endblock %}
</head>
<body>
    {% trans_default_domain 'app' %}
    {% block headbar %}
        {% include "admin/parts/headbar.html.twig" %}
    {% endblock %}
    <div class="container-fluid">
        <div class="row">
            {{ knp_menu_render('admin') }}
            <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
                {% block breadcrumb %}{{ wo_render_breadcrumbs({ viewTemplate: "admin/parts/breadcrumbs.html.twig" }) }}{% endblock %}
                {% include 'admin/parts/flashes.html.twig' %}

                <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-2 border-bottom">
                    <h1 class="h2">{% block mainTitle %}{% endblock %}</h1>
                    {% block options %}{% endblock %}
                </div>
                {% block content %}{% endblock %}
            </main>
        </div>
    </div>
    {% block javascripts %}
        {{ encore_entry_script_tags('admin') }}
    {% endblock %}
</body>
</html>
